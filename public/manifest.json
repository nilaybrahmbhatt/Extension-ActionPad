{
  "manifest_version": 3,
  "name": "WritingPad - Quick Notes, Powerful Results",
  "version": "1.0",
  "description": "Just open a new tab and throw in your tasks, notes, links, and pics",
  "permissions": ["storage", "bookmarks", "contextMenus"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "eventPage.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
